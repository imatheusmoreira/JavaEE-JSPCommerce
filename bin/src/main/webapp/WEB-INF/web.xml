<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FormMail</display-name>
    <servlet-name>FormMail</servlet-name>
    <servlet-class>br.edu.ifce.servlets.FormMail</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>CheckCredentials</display-name>
    <servlet-name>CheckCredentials</servlet-name>
    <servlet-class>br.edu.ifce.servlets.CheckCredentials</servlet-class>
    <init-param>
      <param-name>admin</param-name>
      <param-value>admin</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description></description>
    <display-name>SendMail</display-name>
    <servlet-name>SendMail</servlet-name>
    <servlet-class>br.edu.ifce.servlets.SendMail</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ResumoEmailsEnviados</display-name>
    <servlet-name>ResumoEmailsEnviados</servlet-name>
    <servlet-class>br.edu.ifce.servlets.ResumoEmailsEnviados</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>Logout</display-name>
    <servlet-name>Logout</servlet-name>
    <servlet-class>br.edu.ifce.servlets.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckCredentials</servlet-name>
    <url-pattern>/CheckCredentials</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FormMail</servlet-name>
    <url-pattern>/FormMail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SendMail</servlet-name>
    <url-pattern>/SendMail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResumoEmailsEnviados</servlet-name>
    <url-pattern>/ResumoEmailsEnviados</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
</web-app>